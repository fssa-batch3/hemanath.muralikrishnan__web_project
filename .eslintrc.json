{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "error",
        "no-console": "off",
        "eqeqeq": "error",
        "strict": "off"
    }
}